<Window x:Class="HealthCareManagement.ViewPatientsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="View Patients" Height="300" Width="400">
    <Grid>
        <ListBox x:Name="lbPatients" Margin="10"/>
        <Button Content="View Details" Click="ViewDetails_Click" Margin="10,220,10,10" VerticalAlignment="Bottom"/>
        <Button Content="Back" Click="Back_Click" Margin="10,250,10,10" VerticalAlignment="Bottom"/>
    </Grid>
</Window>
