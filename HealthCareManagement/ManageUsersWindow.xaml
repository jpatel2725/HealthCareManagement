<Window x:Class="HealthCareManagement.ManageUsersWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Manage Users" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Add User" Click="AddUser_Click" Margin="5"/>
            <Button Content="Edit User" Click="EditUser_Click" Margin="5"/>
            <Button Content="Delete User" Click="DeleteUser_Click" Margin="5"/>
            <Button Content="Back" Click="Back_Click" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
