<Window x:Class="HealthCareManagement.AdminDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Admin Dashboard" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Manage Users" Click="ManageUsers_Click" Margin="5"/>
            <Button Content="Add Doctors" Click="AddDoctors_Click" Margin="5"/>
            <Button Content="Logout" Click="Logout_Click" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
