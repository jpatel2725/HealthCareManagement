<Window x:Class="HealthCareManagement.ViewPrescriptionsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="View Prescriptions" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="List of Prescriptions" Margin="5"/>
            <ListBox x:Name="lbPrescriptions" Width="400" Height="200" Margin="5"/>
            <Button Content="Back" Click="Back_Click" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
