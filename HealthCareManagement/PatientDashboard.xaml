<Window x:Class="HealthCareManagement.PatientDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Patient Dashboard" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Manage Appointment" Click="ManageAppointment_Click" Margin="5"/>
            <Button Content="View Prescriptions" Click="ViewPrescriptions_Click" Margin="5"/>
            <Button Content="Profile" Click="Profile_Click" Margin="5"/>
            <Button Content="Logout" Click="Logout_Click" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
